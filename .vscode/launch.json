    {
      "type": "node",
      "request": "launch",
      "name": "Start debug",
      "program": "${workspaceRoot}/node_modules/.bin/webpack-dev-server",
      "runtimeArgs": [
        "--nolazy"
      ],
      "args": [
        "--config",
        ".config/debug.config.js"
      ],
      "cwd": "${workspaceRoot}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Build debug",
      "program": "${workspaceRoot}/node_modules/.bin/webpack",
      "runtimeArgs": [
        "--nolazy"
      ],
      "args": [
        "--config",
        ".config/debug.config.js"
      ],
      "cwd": "${workspaceRoot}"
    }
  ]
